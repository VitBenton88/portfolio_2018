<div class="container-fluid content">
  <div class="row">

    {{> admin/menus/main menu-content=true}}

    <div class="col-11">
      <div class="row">
        {{> admin/notifications}}

        {{> admin/heading heading='Content'}}

        <div class="container">
            <div class="section-head alert alert-primary d-flex justify-content-between align-items-center" role="alert" data-toggle="collapse" href="#navigation">
              Your Navigation
              <div class="section-toggle-controls">
                <i class="fa fa-expand toggle-control" aria-hidden="true"></i>
                <i class="fa fa-minus toggle-control" aria-hidden="true"></i>
              </div>
            </div>

            <div id="navigation" class="collapse">
              {{> admin/navigation}}
            </div>
        </div>

        <div class="container">
            <div class="section-head alert alert-success d-flex justify-content-between align-items-center" role="alert" data-toggle="collapse" href="#introduction">
              Your Introduction
              <div class="section-toggle-controls">
                <i class="fa fa-expand toggle-control" aria-hidden="true"></i>
                <i class="fa fa-minus toggle-control" aria-hidden="true"></i>
              </div>
            </div>

            <div id="introduction" class="collapse">
              {{> admin/content/introduction}}
            </div>
        </div>

        <div class="container">
            <div class="section-head alert alert-info d-flex justify-content-between align-items-center" role="alert" data-toggle="collapse" href="#about">
              Your About
              <div class="section-toggle-controls">
                <i class="fa fa-expand toggle-control" aria-hidden="true"></i>
                <i class="fa fa-minus toggle-control" aria-hidden="true"></i>
              </div>
            </div>

            <div id="about" class="collapse">
              {{> admin/content/about}}
            </div>
        </div>

        <div class="container">
            <div class="section-head alert alert-warning d-flex justify-content-between align-items-center" role="alert" data-toggle="collapse" href="#technology">
              Your Technologies
              <div class="section-toggle-controls">
                <i class="fa fa-expand toggle-control" aria-hidden="true"></i>
                <i class="fa fa-minus toggle-control" aria-hidden="true"></i>
              </div>
            </div>

            <div id="technology" class="collapse">
              {{> admin/content/technology}}
            </div>
        </div>

        <div class="container">
            <div class="section-head alert alert-secondary d-flex justify-content-between align-items-center" role="alert" data-toggle="collapse" href="#portfolio">
              Your Portfolio
              <div class="section-toggle-controls">
                <i class="fa fa-expand toggle-control" aria-hidden="true"></i>
                <i class="fa fa-minus toggle-control" aria-hidden="true"></i>
              </div>
            </div>

            <div id="portfolio" class="collapse">
              {{> admin/content/portfolio}}
            </div>
        </div>

        <div class="container">
            <div class="section-head alert alert-danger d-flex justify-content-between align-items-center" role="alert" data-toggle="collapse" href="#resume">
              Your Resume
              <div class="section-toggle-controls">
                <i class="fa fa-expand toggle-control" aria-hidden="true"></i>
                <i class="fa fa-minus toggle-control" aria-hidden="true"></i>
              </div>
            </div>

            <div id="resume" class="collapse">
              {{> admin/content/resume}}
            </div>
        </div>
      </div>
    </div>
  </div>
</div>